var am9wb={
  appkey  : "2140610257",
  statuses:[],
  statuses_add:function(data){
    this.statuses = data;
    data
  },
  statuses_get:function(id){
    var res;
    $.each(this.statuses, function (i, x) {
      if(x.id==id){
        res=x;
        return false;
      }
      if((x.retweeted_status) && (x.retweeted_status.id==id)){
        res=x.retweeted_status;
        return false;
      }
    });
    return res
  }
};




